{
    "name": "Video Downloader",
    "version": "0.1",
    "manifest_version": 2,
    "web_accessible_resources": ["*.png"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Video Downloader"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": ["tabs"]
  }